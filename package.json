{
  "name": "new-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.5.2",
    "@types/jest": "24.0.20",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/redux-actions": "^2.6.1",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "final-form": "4.18.5",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "moment": "2.24.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.0",
    "promise-sequential": "1.1.1",
    "react": "16.11.0",
    "react-app-rewired": "2.1.4",
    "react-dom": "16.11.0",
    "react-final-form": "6.3.0",
    "react-redux": "7.1.1",
    "react-router5": "7.1.0",
    "react-scripts": "3.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "redux": "4.0.4",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "1.1.1",
    "reselect": "4.0.0",
    "router5": "7.0.2",
    "router5-helpers": "7.0.1",
    "router5-plugin-browser": "7.0.2",
    "router5-plugin-logger": "7.0.2",
    "typescript": "3.6.4"
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "build:front": "cross-env NODE_ENV=production react-app-rewired build",
    "test:watch": "jest --watch",
    "test:ci": "set CI=true && jest",
    "lint-scripts": "eslint . --ext .ts,.js,.tsx",
    "lint-scripts-fix": "eslint . --ext .ts,.js,.tsx --fix",
    "lint-styles": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint-styles-fix": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\" --fix",
    "lint-fix": "npm run lint-scripts-fix && npm run lint-styles-fix",
    "lint": "git add . && lint-staged",
    "commit": "npm run lint && git cz && git push",
    "stop:win": "taskkill /F /IM node.exe",
    "stop:lin": "killall node",
    "heroku-postbuild": "npm run build:front",
    "start": "node ./server"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "cz-customizable": "6.2.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "husky": "3.0.9",
    "lint-staged": "13.2.2",
    "prettier": "1.18.2",
    "stylelint": "11.1.1",
    "stylelint-config-recommended-scss": "4.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "3.1.1",
    "stylelint-scss": "3.12.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}
